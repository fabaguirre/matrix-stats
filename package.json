{
  "name": "matrix-stats",
  "version": "1.0.0",
  "description": "API that receives matrix data, computes statistical metrics including max, min, average, and checks for diagonal matrices",
  "main": "index.js",
  "scripts": {
    "build": "tspc",
    "start": "node dist/index.js",
    "build:start": "npm run build && npm run start",
    "dev": "npx tsx watch src/index.ts",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write ."
  },
  "keywords": [
    "matrix",
    "stats"
  ],
  "author": "fabos",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "4.17.21",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.5",
    "ts-patch": "^3.2.0",
    "tsx": "^4.7.1",
    "typescript": "5.4.2",
    "typescript-transform-paths": "^3.4.7"
  },
  "dependencies": {
    "express": "^4.19.2",
    "valibot": "^0.37.0"
  }
}
